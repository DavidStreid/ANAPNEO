{
  "name": "anapneo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=dev",
    "build": "ng build --configuration=prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "log": "mkdir -p ./log",
    "setup": "npm install && npm run log",
    "dev-client": "ng serve --configuration=dev >> ./log/client.log 2>&1 &",
    "dev-server": "nodemon ./src/server/server.js >> ./log/server.log 2>&1 &",
    "dev-db": "mongod >> ./log/db.log 2>&1 &",
    "go": "npm run setup && npm run dev-db && npm run dev-client && npm run dev-server && echo Go_to_localhost::4200",
    "build-client": "ng build anapneo-lib --configuration=prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "rxjs": "~6.2.0",
    "rxjs-compat": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.1",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~7.1.1",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@types/jasmine": "~3.3.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.12",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
